{
  "functions": {
    "source": "functions"
  },
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "/q-local-*",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/octet-stream"
          },
          {
            "key": "Content-Disposition",
            "value": "attachment"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/v1/query",
        "function": "v1Query"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
